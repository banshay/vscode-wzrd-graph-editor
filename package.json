{
  "name": "vscode-wzrd-graph-editor",
  "displayName": "vscode-wzrd-graph-editor",
  "description": "",
  "version": "0.0.10",
  "publisher": "wzrd-graph",
  "repository": {
    "url": "https://github.com/banshay/vscode-wzrd-graph-editor"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "browser": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-wzrd-graph-editor.helloWorld",
        "title": "Hello World2"
      }
    ],
    "customEditors": [
      {
        "viewType": "wzrd.editor",
        "displayName": "Wzrd Editor",
        "selector": [
          {
            "filenamePattern": "*.wzrd"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@banshay/graph-editor": "file:../graph_editor/graph_editor",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "copy-webpack-plugin": "^11.0.0"
  }
}
